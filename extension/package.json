{
  "name": "prompt-manna-extension",
  "displayName": "Prompt Manna",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "echo 'no lint configured'",
    "export-icons": "node scripts/export-icons.js",
    "verify-icons": "node scripts/verify-icons.js",
    "generate-svgs": "node scripts/generate-svg-icons.js",
    "compare-svg": "node scripts/compare-svg-to-prototype.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "plasmo": "^0.90.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.276",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "image-size": "^1.2.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.47",
    "puppeteer": "^24.25.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.2"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "contextMenus",
      "storage"
    ],
    "externally_connectable": {
      "matches": [
        "http://localhost:3000/*",
        "https://promptmanna.app/*"
      ]
    }
  }
}
